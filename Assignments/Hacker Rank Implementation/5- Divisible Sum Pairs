#include <iostream>
#include<vector>
using namespace std;
int main() {
    
    int n,k;
    cin>>n>>k;
    vector<int>v(n);
    
    for(int i =0; i<n ;i++){
        cin>>v[i];
    }
    
    int count = 0;
    for(int i = 0; i<v.size() ;i++){
        
        for(int j = i+1; j<v.size(); j++){
           if((v[i]+v[j])%k==0){
               count++;
           }
        }
    }
    
    cout<<count;
    return 0;
}
